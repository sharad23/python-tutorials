class Student(object):
   test = 10;
   def __init__(self,name,roll_no):
      self.name =  name
      self.roll_no = roll_no
   
   def inst(self):
      print self.name+''+self.roll_no
   
   def retobj(self):
   	  print self
      
   def hobnob(self):
       print self.test
  
   @classmethod
   def methclass(cls):
      print cls.test

  

t1 = Student('sharad','66088')
print t1
print t1.hobnob


